//14581번 팬들에게 둘러싸인 홍준

import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		String a = sc.next();
		System.out.println(":fan::fan::fan:");
		System.out.println(":fan::" + a + "::fan:");
		System.out.println(":fan::fan::fan:");
		sc.close();
	}
}
